<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="app.prime.rest">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d|%-5p|%-10t|%m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="app.prime" level="info" additivity="false">
            <AppenderRef ref="STDOUT"/>
        </Logger>
        <Root level="info">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>
